{
  "name": "electron-app",
  "productName": "electron-app",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": "src/main.js",
  "scripts": {
    "start:angular": "cd ../site && ng serve",
    "start:electron": "cross-env NODE_ENV=development electron-forge start",
    "start": "concurrently \"npm run start:angular\" \"npm run start:electron\"",
    "package": "cross-env NODE_ENV=production electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\"",
    "build:angular": "cd ../site && ng build --configuration production --output-path=../electron-app/build/web",
    "package:electron": "cd ../electron-app && electron-forge package",
    "make:electron": "cd ../electron-app && electron-forge make",
    "deploy": "npm run build:angular && npm run make:electron"
  },
  "keywords": [],
  "author": {
    "name": "MiguelBCosta",
    "email": "miguelbertemes@hotmail.com"
  },
  "license": "MIT",
  "dependencies": {
    "better-sqlite3": "^11.10.0",
    "electron-squirrel-startup": "^1.0.1",
    "eventemitter2": "^6.4.9",
    "knex": "^3.1.0",
    "objection": "^3.1.5"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.8.1",
    "@electron-forge/maker-deb": "^7.8.1",
    "@electron-forge/maker-rpm": "^7.8.1",
    "@electron-forge/maker-squirrel": "^7.8.1",
    "@electron-forge/maker-zip": "^7.8.1",
    "@electron-forge/plugin-auto-unpack-natives": "^7.8.1",
    "@electron-forge/plugin-fuses": "^7.8.1",
    "@electron/fuses": "^1.8.0",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "electron": "^36.2.1"
  }
}
